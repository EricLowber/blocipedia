
<div class="row">
<div class="col-md-12">
  <h2><%= @wiki.title %></h2>
  <h3><strong>Update Collaborators</strong></h3>
</div>
</div>
<%= form_for(@wiki) do |f| %>
<div class="row">
  <div class="col-md-2">
   <%= f.submit "Update", class: 'btn btn-primary' %>
    </div>
  <div class="col-md-8">
       <table class="table table-hover">
        <tr>
           <td></td>
           <td>Name</td>
        </tr>
            <tr>
           <%= hidden_field_tag "wiki[user_ids][]", nil %>
           <% @users.each do |user| %>
              <td><%= check_box_tag "wiki[user_ids][]", user.id, @wiki.user_ids.include?(user.id), id: dom_id(user) %></td>
              <td><%= label_tag dom_id(user), user.name %></td>
              <td><%= user.id %></td>
            </tr>
           <% end %>
        </table>
          <% end %>
          </br>
          </br>
      </div>
    </div>
